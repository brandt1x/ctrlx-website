// Globals
const string WeaponChoices[] = {"AK","TOMMY","MP5","SKS","SAR","M249","LR","LMG","HSMG","M39","SMG","P2","HCAL","M92","GLK","PYT","REVO","NG","MG","AKMZB"};
int XLen = 3;           // number of columns
int ListLen = 19;       // last index
int line_offset = 10;   // starting y position
int row_height = 20;    // vertical spacing per row
int CurrentWeapon = 0;  // selected weapon

// loop counters and positions
int i;
int x_pos;
int y_pos;
int scroll_row;
int weapon_row;
int weapon_col;

function Weapons()
{
    cls_oled(OLED_BLACK);

    i = 0; // initialize counter
    scroll_row = CurrentWeapon / XLen;

    while(i <= ListLen)
    {
        weapon_row = i / XLen;
        weapon_col = i % XLen;

        // left-aligned x positions
        x_pos = 2 + weapon_col * 40;

        // y position relative to scroll
        y_pos = line_offset + (weapon_row - scroll_row) * row_height;

        // only draw if visible
        if(y_pos >= 0 && y_pos <= 64 - row_height)
        {
            if(i == CurrentWeapon)
                print(x_pos, y_pos, OLED_BLACK, OLED_FONT_SMALL, WeaponChoices[i]);
            else
                print(x_pos, y_pos, OLED_BLACK, OLED_FONT_MEDIUM, WeaponChoices[i]);
        }

        i = i + 1; // increment
    }
}

const string Deadzones[]= {"ADS X: ", "ADS Y: ", "X:", "Y:"} 
const string Automation[] = {""};
function SETTINGS()
{
	print(50- (3 * 5), 0, OLED_BLACK, OLED_FONT_MEDIUM, Deadzones[0]);
	print(90- (3 * 3), 0, OLED_BLACK, OLED_FONT_MEDIUM, NumberSet[CurrentDeadzoneX]);
	
	print(50- (3 * 5), 12, OLED_BLACK, OLED_FONT_MEDIUM, Deadzones[1]);
	print(90- (3 * 3), 12, OLED_BLACK, OLED_FONT_MEDIUM, NumberSet[CurrentDeadzoneY]);
	
	print(64- (3 * 12), 30, OLED_BLACK, OLED_FONT_MEDIUM, Automation[0]);
	if (get_controller() == PIO_XB1)
	{
		print(64- (3 * 7), 50, OLED_BLACK, OLED_FONT_MEDIUM, Automation[1]);
	}
	if (get_controller() == PIO_PS5)
	{
		print(64- (3 * 10), 50, OLED_BLACK, OLED_FONT_MEDIUM, Automation[2]);
	}
	
	
	
	
}



int isDoneCovering = FALSE
int TempY, TempX
function CoverScreen()

{

if (timer > 1 && TempY < 64)

{

line_oled(0, TempY, 124, TempY, 2, 1)

TempY += 2;

timer = 0;

}

if (TempY >= 64)

{

isDoneCovering = TRUE

}

}



int IsFinishedWithScreenChange = FALSE

function ReleaseScreen()

{



if (timer > 1 && TempY > -1)

{

line_oled(0, TempY, 124, TempY, 2, OLED_BLACK)

TempY -= 2;

timer = 0;

}

if (TempY < 0)

{


isDoneCovering = FALSE;

timer = 0


IsFinishedWithScreenChange = TRUE;



}

}

int Is_Completly_Finished = FALSE;

function DetermineScreenUpdate()

{



if (!isDoneCovering && !IsFinishedWithScreenChange)

{

CoverScreen();

}

if (isDoneCovering && !IsFinishedWithScreenChange)

{

ReleaseScreen();

}

if (IsFinishedWithScreenChange && !ClearScreen)

{

ClearScreen = FALSE

}


}


int y, x, index;

function DrawBitmap(x_start, y_start, width, height, WHICH) {

for (y = 0; y < height; y = y + 1)

{
	
	for (x = 0; x < width; x = x + 1)
	
	{
	
		// Calculate the 1D index from the 2D coordinates
		
		index = (y * width) + x;
		
		
		// Draw a pixel only if the value in the array is 1 (White)
		
		
		if (Intro[index] == 1 )
		
