int spinning = FALSE;

main {
    // spin buttons
    if (event_press(XB1_RS) && event_press(XB1_LS)) {
        spinning = !spinning;
    }

    // Apply spin
     if (spinning) {
        set_val(XB1_RX, 100);   // Spin right
        set_val(XB1_RY, 100);     // Neutral vertical stick
        set_val(XB1_LY, 100);   // Move forward
        set_val(XB1_LX, 100);     // No horizontal movement
        set_val(XB1_A, 100);    // Jump
    }
    // Otherwise, leave sticks/buttons free for normal use
}



