int CurrentScope = 0

int CurrentAdded, InvdeadzoneY, RY_, LX_;
function AddToCombosRY()
{
	CurrentAdded = 0;
	InvdeadzoneY = CurrentDeadzoneY * -1
	RY_ = get_val(RY);
	LX_ = get_val(LX); //LEFT = -    right = +
	
	//Movement Control recoil through Variable
	if (LX_< -6 || LX_ > 6)
	{
		if (CurrentWeapon == 0 && CurrentStatus % 2 == 0)
		{
			CurrentAdded = CurrentAdded + 5;
		}
		else if (CurrentWeapon == 1 && CurrentStatus % 2 == 0)
		{
			CurrentAdded = CurrentAdded + 3 ;
		}else if (CurrentWeapon == 3 && CurrentStatus % 2 == 0)
		{
			CurrentAdded = CurrentAdded + 10 ;
		}
		
		else if (CurrentWeapon == 4 && CurrentStatus % 2 == 0 && CurrentScope == 0)//sar standing
		{
			CurrentAdded = CurrentAdded + 8 ;
		}
		else if (CurrentWeapon == 4 && CurrentStatus % 2 == 1&& CurrentScope == 0)//sar crouche
		{
			CurrentAdded = CurrentAdded + 3 ;
		}
		else if (CurrentWeapon == 4 && CurrentStatus % 2 == 1 && CurrentScope == 1)//sar crouche 4X
		{
			CurrentAdded = CurrentAdded + 9 ;
		}
		
		
		else if (CurrentWeapon == 5 && CurrentStatus % 2 == 1&& CurrentScope == 0)//M2 crouche
		{
			CurrentAdded = CurrentAdded + 8 ;
		}
		else if (CurrentWeapon == 5 && CurrentStatus % 2 == 0 && CurrentScope == 0)//M2 Standed
		{
			CurrentAdded = CurrentAdded + 22 ;
		}
		else
		{	
		CurrentAdded = CurrentAdded + 1;
		}
	}

	//All before this is logic LEAVE ALONE	

	if (CurrentSSX == 0)
	{
		if ( RY_ > CurrentDeadzoneY && CurrentSSX == 0 && CurrentStatus == 0) // Held Down
		{
			return (RY_ - CurrentDeadzoneY) / 1.5
		}
		if (abs(RY_) > CurrentDeadzoneY && CurrentSSX == 0 && CurrentStatus == 0)
		{
			return (RY_ * 1.5 + CurrentDeadzoneY)   
		}
		if ( RY_ > CurrentDeadzoneY && CurrentSSX == 0 && CurrentStatus == 1) // Held Down
		{
			return (RY_ - CurrentDeadzoneY) 
		}
		if (abs(RY_) > CurrentDeadzoneY && CurrentSSX == 0 && CurrentStatus == 1)
		{
			return (RY_ * 1.5 + CurrentDeadzoneY)
		}
	}
	else
	{
		if ( RY_ > CurrentDeadzoneY) // Held Down
	
		{
			return (RY_ - CurrentDeadzoneY) / 2
		}
		if (abs(RY_) > CurrentDeadzoneY)
		{
			return (RY_ + CurrentDeadzoneY)
		}
	}
	
	
	
	
	//All before this is logic LEAVE ALONE
	if (CurrentStatus % 2 == 1)
	{
		CurrentAdded = CurrentAdded + 1
	}
	
	
	

	return CurrentAdded
}
int currentDivider;
function AddToCombosRX()
{
	if (CurrentSSX == 0)
	{
		currentDivider = 1
	}
	else if (CurrentSSX == 1)
	{
		currentDivider = 1
	}
	if (CurrentSSX == 0)
	{
		if(get_val(RX) > CurrentDeadzoneX && CurrentStatus == 0 && CurrentSSX == 0) //Hold right
		{		
			return (get_val(RX)* 10/9 - CurrentDeadzoneX) / currentDivider //100 - 11
		}
		if(get_val(RX) < (CurrentDeadzoneX*-1) && CurrentStatus == 0 && CurrentSSX == 0) // Hold Left
		{
			return (get_val(RX) + (CurrentDeadzoneX)) / currentDivider //-100 + 11 
		}
		
		//crouch
		if(get_val(RX) > CurrentDeadzoneX && CurrentStatus == 1 && CurrentSSX == 0) //Hold right
		{		
			return (get_val(RX)* 10/9 - CurrentDeadzoneX)/ currentDivider //100 - 11
		}
		if(get_val(RX) < (CurrentDeadzoneX*-1) && CurrentStatus == 1 && CurrentSSX == 0) // Hold Left
		{
			return (get_val(RX) + (CurrentDeadzoneX))/ currentDivider //-100 + 11 
		}

	}else
	{
	
		if(get_val(RX) > CurrentDeadzoneX && CurrentStatus == 0 && CurrentSSX == 1) //Hold right
		{		
			return (get_val(RX)* 10/9 - CurrentDeadzoneX) / 2 
		}
		if(get_val(RX) < (CurrentDeadzoneX*-1) && CurrentStatus == 0 && CurrentSSX == 1) // Hold Left
		{
			return (get_val(RX) + (CurrentDeadzoneX)) / 2 //-100 + 11 
		}
		
		//crouch
		if(get_val(RX) > CurrentDeadzoneX && CurrentStatus == 1 && CurrentSSX == 1) //Hold right
		{		
			return (get_val(RX) - CurrentDeadzoneX)/ 2 //100 - 11
		}
		if(get_val(RX) < (CurrentDeadzoneX*-1) && CurrentStatus == 1 && CurrentSSX == 1) // Hold Left
		{
			return (get_val(RX) + (CurrentDeadzoneX))/ 2 //-100 + 11 
		}
	}
	
	
	
	
	
	return 0;

}



